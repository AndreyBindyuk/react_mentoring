    "react-dom": "^16.8.6",
    "react-rater": "^5.0.5",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-star-rating": "^1.4.2",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "webpack": "^4.30.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
